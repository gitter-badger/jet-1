{
    "tasks": [
        {
            "type": "shell",
            "label": "Run make jetc-dbg",
            "command": "make",
            "args": [
                "jetc-dbg"
            ],
            "argse": [
                "-g",
                "${file}",
                "-Imodules",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}